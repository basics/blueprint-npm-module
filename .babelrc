{
  "env": {
    "development": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "corejs": "2",
            "useBuiltIns": "usage"
          }
        ]
      ],
      "plugins": [
        "istanbul"
      ]
    },
    "production": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "corejs": "2",
            "useBuiltIns": "usage"
          }
        ],
        "minify"
      ],
      "plugins": [
        "istanbul"
      ]
    }
  }
}
